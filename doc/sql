CREATE DATABASE printcalc;

use printcalc;

CREATE TABLE pricingrules (
id INT(6) UNSIGNED AUTO_INCREMENT PRIMARY KEY,
papertype VARCHAR(10) NOT NULL,
printtype BOOLEAN NOT NULL,
blackandwhiteprice INT(10) NOT NULL,
colourprice INT(10) NOT NULL
)

INSERT INTO pricingrules2(papertype, printtype, blackandwhiteprice, colourprice) VALUES('A4', false, 15, 25);
INSERT INTO pricingrules2(papertype, printtype, blackandwhiteprice, colourprice) VALUES('A4', true, 10, 20);